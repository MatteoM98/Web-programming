<div id="alert-message">
   <% if(message) { %>
     <div class="alert alert-<%=messageType%> alert-dismissible fade show" role="success">
       <span><%= message %></span> 
       <button type="button" class="close" data-dismiss="alert" aria-label="Close">
           <span aria-hidden="true">&times;</span>
       </button>
     </div>
   <% } %>
</div>